{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AllowEC2ToAssumeRole",
        "Effect": "Allow",
        "Principal": {
          "Service": "ec2.amazonaws.com"
        },
        "Action": "sts:AssumeRole"
      },
      {
        "Sid": "AllowAWSBackupsToAssumeRole",
        "Effect": "Allow",
        "Principal": {
          "Service": "backup.amazonaws.com"
        },
        "Action": "sts:AssumeRole"
      }
    ]
  }
