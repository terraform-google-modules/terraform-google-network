{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowBucketManagement",
            "Effect": "Allow",
            "Action": [
                "s3:GetBucketLocation",
                "s3:ListBucket",
                "s3:GetBucketCORS",
                "s3:ListBucketVersions",
                "s3:GetBucketLogging",
                "s3:GetLifecycleConfiguration",
                "s3:PutLifecycleConfiguration"
            ],
            "Resource": ${jsonencode(arn_list)}
        },
        {
            "Sid": "AllowObjectManagement",
            "Effect": "Allow",
            "Action": [
                "s3:DeleteObjectVersion",
                "s3:GetObjectVersion",
                "s3:RestoreObject",
                "s3:GetObject",
                "s3:PutObject",
                "s3:PutObjectAcl",
                "s3:DeleteObject"
            ],
            "Resource": ${jsonencode([for arn in arn_list : "${arn}/*"])}
        }
    ]
}
