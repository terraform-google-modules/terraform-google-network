{
    "Version": "2012-10-17",
    "Statement": [
        {
          "Sid": "CreateCloudwatchAlarms",
          "Effect": "Allow",
          "Action": [
              "SNS:ListTopics",
              "SNS:CreateTopic",
              "SNS:GetTopicAttributes",
              "SNS:ListSubscriptions",
              "SNS:Subscribe",
              "cloudwatch:PutMetricAlarm"
          ],
          "Resource": "*"
        }
    ]
  }
